{
  "name": "steenbag/tubes",
  "description": "A package for auto wiring web services.",
  "authors": [{
    "name": "Andrew Steenbuck",
    "email": "steenbag@live.com"
  }],
  "require": {
    "php": ">=5.6.0",
    "apache/thrift": "0.12.0",
    "phpseclib/phpseclib": "~2.0",
    "namshi/jose": "~7.0",
    "symfony/http-foundation": "~3.4",
    "anahkiasen/underscore-php": "~2.0",
    "nesbot/carbon": "~1.37"
  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev"
  },
  "suggest": {
    "illuminate/support": "5.*",
    "laravel/framework": "5.*",
    "php-activerecord/php-activerecord": "~1.0",
    "laravel-doctrine/orm": "^1.3"
  },
  "autoload": {
    "classmap": [
      "src/migrations",
      "src/controllers",
      "tests"
    ],
    "psr-0": {
      "Steenbag\\Tubes\\Tests": "tests/",
      "Steenbag\\Tubes\\": "src/"
    }
  },
  "minimum-stability": "dev"
}
